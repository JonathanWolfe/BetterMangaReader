{
  "name": "BetterMangaReader",
  "version": "0.3.0",
  "description": "Keep track of your manga, be notified of updates, and have a better reading experience",
  "repository": {
    "type": "git",
    "url": "git://github.com/JonathanWolfe/BetterMangaReader.git"
  },
  "browser": {
    "jquery": "./node_modules/jquery/dist/jquery.min.js"
  },
  "browserify-shim": {
    "jquery": "$"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "devDependencies": {
    "browserify": "*",
    "browserify-shim": "*",
    "gulp": "*",
    "gulp-uglify": "*",
    "jquery": "*",
    "streamify": "*",
    "vinyl-buffer": "*",
    "vinyl-source-stream": "*",
    "wurl": "*"
  }
}
